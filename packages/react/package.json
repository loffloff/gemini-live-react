{
  "name": "gemini-live-react",
  "version": "0.4.0",
  "description": "React hook for Google Gemini Live API with real-time bidirectional voice streaming",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "gemini",
    "google",
    "voice",
    "audio",
    "real-time",
    "streaming",
    "react",
    "hook",
    "speech",
    "ai",
    "live-api",
    "session-recording",
    "workflow",
    "automation",
    "browser-control",
    "element-detection",
    "voice-assistant",
    "conversational-ai"
  ],
  "author": "loffloff",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/loffloff/gemini-live-react"
  },
  "homepage": "https://github.com/loffloff/gemini-live-react#readme",
  "bugs": {
    "url": "https://github.com/loffloff/gemini-live-react/issues"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "@ricky0123/vad-web": ">=0.0.7"
  },
  "peerDependenciesMeta": {
    "@ricky0123/vad-web": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
